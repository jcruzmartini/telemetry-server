<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.techner.tau.server.domain.model.EventStatus" table="EVENT_STATUS">
		<id column="ID_EVENT_STATUS" name="id">
			<generator class="native" />
		</id>
		<property column="CODE" name="code" type="string" />
		<property column="DESCRIPTION" name="description" type="string" />
        <property column="IS_CRITICAL" name="critical" type="boolean" />
	</class>
</hibernate-mapping>
